  "name": "email",
  "templates": {
    "snippet": "<h4 class='title'>{{{subject}}}</h4>\n  <p>\n    <span>{{ date }}</span>\n    <span><small>{{{from}}} &rarr; {{{to}}}</small></span>\n {{{ snippet }}} </p>\n  ",
    "full": "<article class=\"email-projection\">\n <header>\n     <h1>{{{subject}}}</h1>\n     <p>{{{ date }}}</p>\n     <p>From: <strong><a href=\"anyfetch://search/{{from}}\">{{{from}}}</a></strong></p>\n     <p>To: <strong><a href=\"anyfetch://search/{{to}}\">{{{to}}}</a></strong></p>\n <hr></header>\n\n <main>\n       {{{html}}}\n </main>\n</article>\n",
    "title": "{{{ subject }}}"
  }
}
